<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ReviewControlText">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="ReviewControlTextBlack">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="ReviewControlGroup">
        <item name="android:gravity">bottom|center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="SettingPanelItem">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/setting_icon_bg</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:drawableCompactMode">true</item>
    </style>
</resources>
